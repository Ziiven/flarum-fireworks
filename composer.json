{
    "name": "ziiven/flarum-fireworks",
    "description": "A Flarum extension. Intergrat FireworkJS to Flarum",
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": "^1.0"
    },
    "support": {
        "issues": "https://github.com/Ziiven/flarum-fireworks/issues",
        "source": "https://github.com/Ziiven/flarum-fireworks"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Ziven\\fireworks\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Ziven"
        }
    ],
    "extra": {
        "flarum-extension": {
            "title": "Ziven Firewroks",
            "icon": {
                "name": "fas fa-fire",
                "backgroundColor": "#238c59",
                "color": "#fff"
            }
        }
    }
}